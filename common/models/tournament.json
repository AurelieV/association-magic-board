{
  "name": "Tournament",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "ranks": {
      "type": "hasMany",
      "model": "Rank"
    },
    "members": {
      "type": "hasMany",
      "model": "Member",
      "through": "Rank"
    }
  },
  "acls": [],
  "methods": []
}
